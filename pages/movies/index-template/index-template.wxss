@import '/pages/movies/singleM-template/singleM-template.wxss';
.m-section {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-bottom: 30rpx;
  padding: 20rpx 0;
}
.tel-tool {
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  justify-content: space-between;
}
.cat-title {
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.cat-more {
  height: 40rpx;
  line-height: 40rpx;
}
.more {
  font-size: 28rpx;
  margin-right: 10rpx;
  color: #1f4ba5;
}
.arrow-right {
  width: 9rpx;
  height: 16rpx;

}
.m-content {
  display: flex;
  flex-direction: row;
  padding-left: 20rpx;
  padding-right: 20rpx;
  justify-content: space-between;
}